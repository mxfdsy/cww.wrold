<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:int="http://www.springframework.org/schema/integration"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:int-amqp="http://www.springframework.org/schema/integration/amqp"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/integration
        http://www.springframework.org/schema/integration/spring-integration.xsd
        http://www.springframework.org/schema/integration/amqp
        http://www.springframework.org/schema/integration/amqp/spring-integration-amqp.xsd">


    <int:channel id="test_send_channel"/>
    <int-amqp:outbound-channel-adapter channel="test_send_channel"
                                       routing-key="cww.test.queue"
                                       exchange-name="test.exchange"/>

    <int:channel id="test_receiver_channel"/>
    <int-amqp:inbound-channel-adapter channel="test_receiver_channel"
                                      error-channel="amqpAsynFailedMessageChannel"
                                      connection-factory="rabbitConnectionFactory"
                                      queue-names="cww.test.queue"/>
    <int:service-activator ref="testHandler" method="test"
                           input-channel="test_receiver_channel"/>
</beans>